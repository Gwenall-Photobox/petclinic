node('amazon-ecs-docker-default'){
    def cluster = 'aws'
    def zone = 'eu-west-1'
    def project = 'petclinic'
    def docker-repo = ${env.DOCKER_REPO - 'docker.photobox.com'}

    stage 'Checkout'
    git url: "https://github.com/Gwenall-Photobox/${project}.git"

    stage 'Build application Docker image'
    sh(echo kuberctl parameters )
    sh(kubectl create  -f kubernetes )
}
