node('amazon-ecs-docker-default'){
    def cluster = 'aws'
    def zone = 'eu-west-1'
    def project = 'petclinic'
    //def docker-repo = ${env.DOCKER_REPO - 'docker.photobox.com'}

    stage 'Checkout'
    git "git@github.com:Gwenall-Photobox/petclinic.git" 

    stage 'Build application Docker image'
    stage 'Load a file from GitHub'
    def helloworld = fileLoader.fromGit('examples/fileLoader/helloworld', 
                    'https://github.com/jenkinsci/workflow-remote-loader-plugin.git', 'master', null, '')
    stage 'Run method from the loaded file'
    helloworld.printHello()


    

